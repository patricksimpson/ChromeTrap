{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon_128.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "cst.css" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "A quick way to capture the current webpage to steeltrap.co",
   "manifest_version": 2,
   "name": "A Steel Trap",
   "permissions": [ "\u003Call_urls\u003E", "tabs", "storage"],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}